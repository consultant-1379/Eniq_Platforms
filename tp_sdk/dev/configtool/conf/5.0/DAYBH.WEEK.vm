## UPDATE DAYBH-table
##

UPDATE $TargetTable.get("DAYBHCLASS")_IN AS dst 
SET 
dst.bhclass = src.bhclass from $SourceTable.get("DAYBHCLASS")_ACT as src
WHERE dst.date_id between $dateid and dateadd(dd,6,$dateid)
and dst.date_id = src.date_id
and dst.bhtype = src.bhtype and
