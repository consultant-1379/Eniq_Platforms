<project name="twampmparser" default="test_all">
    <property name="module.name" value="twampmparser"/>
    <property file="${user.home}/EniqBuild/BH/build.properties"/>
    <property name="ant.common" value="${cc.base}/tools/ant_common"/>
    <property file="${ant.common}/common.properties"/>
    <property name="module.build" value="${module.twampmparser.build}"/>
    <property name="module.base" value="${module.twampmparser.base}"/>
    <property name="module.dependants" value="${module.twampmparser.dependants}"/>
    <import file="${ant.common}/common.xml"/>
    <property name="tests.to.run" value="**/*Test.class"/>

    <path id="twampmparser.lib.classpath">
        <path refid="plaform.lib.classpath"/>
        <path location="${module.common_utilities.pkg}"/>
        <path location="${module.parser.pkg}"/>
        <path location="${module.repository.pkg}"/>
    </path>
    <path id="twampmparser.test.classpath">
        <path refid="platform.test.classpath"/>
        <path refid="twampmparser.lib.classpath"/>
        <path location="${eniq.plat.base}/adapters/dev/twampmparser/"/>
    </path>
</project>