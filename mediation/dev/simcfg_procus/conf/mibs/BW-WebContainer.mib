-- WebContainer worker.......................................................................    

  bwHttpWorkersReset OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
      "When set to a value other than 0, all data under workers are reset to 0."
-- @feature:147586
    ::= { serverResources  13 }

  bwHttpWorkerThreadsBusy OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
      "The number of workers serving the request."
-- @feature:147586
    ::= { workers  1 }
    
  bwHttpWorkerThreadsIdle OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
      "The number of idle workers."
-- @feature:147586
    ::= { workers  2 }  

  bwHttpWorkerThreadsConsecutiveFailedRefresh OBJECT-TYPE
    SYNTAX     Counter32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
      "A counter that indicates the number of successive failed refresh. Upon a successful refresh, it auto-resets to 0."
-- @feature:147586
    ::= { workers  3 }
  
  bwHttpWorkerThreadsUsable OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
      "Number of configured usable worker threads."
-- @feature:147586
    ::= { workers  4 }
  
  bwHttpWorkerThreadsBusyAvg OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
      "Average number of busy worker threads since last reset. That is, every 5 seconds the result is based on the sum of all busy worker threads divided by the number of polls since the last reset."
-- @feature:147586
    ::= { workers  5 }

  bwHttpWorkerThreadsBusyMax OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
      "Maximum number of busy worker threads since last reset"
-- @feature:147586
    ::= { workers  6 }
    
-- WebContainer overhead.......................................................................    

  bwHttpOverheadReset OBJECT-TYPE
    SYNTAX     Gauge32
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
      "When set to a value other than 0, all data under overhead are reset to 0."
-- @feature:147586
   ::= { serverResources  15 }
   
