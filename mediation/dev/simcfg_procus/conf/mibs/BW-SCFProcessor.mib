-- SCF Processor - TCP Framework ........................

  bwSCFProcessorTcpServersStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BwSCFProcessorTcpServersStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains counters for TCP underlying framework for the SCP process."
-- @feature:158606
    ::= { tcp 1 }

  bwSCFProcessorTcpServersStatsEntry OBJECT-TYPE
    SYNTAX      BwSCFProcessorTcpServersStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Definition of the entries for bwSCFProcessorTcpServersStatsTable."
    INDEX       { bwSCFProcessorTcpServersStatsIndex }
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsTable 1 }

  BwSCFProcessorTcpServersStatsEntry ::= SEQUENCE {
    bwSCFProcessorTcpServersStatsIndex                Unsigned32,
    bwSCFProcessorTcpServersName                      DisplayString,
    bwSCFProcessorTcpServersNbConnectionsAccepted     Counter32,
    bwSCFProcessorTcpServersNbConnectionsRefused      Counter32,
    bwSCFProcessorTcpServersNbConnectionsInitiated    Counter32,
    bwSCFProcessorTcpServersNbConnectionsClosed       Counter32,
    bwSCFProcessorTcpServersNbBytesSent               Gauge32,
    bwSCFProcessorTcpServersNbBytesReceived           Gauge32,
    bwSCFProcessorTcpServersOutgoingQueueSize         Gauge32,
    bwSCFProcessorTcpServersIncomingQueueSize         Gauge32,
    bwSCFProcessorTcpServersNbBytesSentSecure         Gauge32,
    bwSCFProcessorTcpServersNbBytesReceivedSecure     Gauge32
  }

  bwSCFProcessorTcpServersStatsIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This integer represents an index in table bwSCFProcessorTcpServersStatsTable. This is the index to a specific TCP server name."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 1 }

  bwSCFProcessorTcpServersName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The name of TCP server - Names are internally defined at at the application level."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 2 }

  bwSCFProcessorTcpServersNbConnectionsAccepted OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of connections accepted by the TCP server."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 3 }

  bwSCFProcessorTcpServersNbConnectionsRefused OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of connections refused by the TCP server."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 4 }

  bwSCFProcessorTcpServersNbConnectionsInitiated OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of connections initiated by the TCP server."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 5 }

  bwSCFProcessorTcpServersNbConnectionsClosed OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of connections closed by the TCP server or remote hosts."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 6 }

  bwSCFProcessorTcpServersNbBytesSent OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of bytes sent by the TCP server."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 7 }

  bwSCFProcessorTcpServersNbBytesReceived OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of bytes received by the TCP server."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 8 }

  bwSCFProcessorTcpServersOutgoingQueueSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of bytes in the outgoing queue."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 9 }

  bwSCFProcessorTcpServersIncomingQueueSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of bytes in the incoming queue."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 10 }

  bwSCFProcessorTcpServersNbBytesSentSecure OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of bytes sent by the TCP server over secure connections."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 11 }

  bwSCFProcessorTcpServersNbBytesReceivedSecure OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter represents the number of bytes received by the TCP server over secure connections."
-- @feature:158606
    ::= { bwSCFProcessorTcpServersStatsEntry 12 }

-- SCF Processor - Common Communication Protocol ........................

  bwSCFProcessorCommonCommStatsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BwSCFProcessorCommonCommStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This table contains counters for BCCT for the SCF Processor process."
-- @feature:158606
    ::= { commonComm 1 }

  bwSCFProcessorCommonCommStatsEntry OBJECT-TYPE
    SYNTAX      BwSCFProcessorCommonCommStatsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Definition of the entries for bwSCFProcessorCommonCommStatsTable."
    INDEX       { bwSCFProcessorCommonCommStatsIndex }
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsTable 1 }

  BwSCFProcessorCommonCommStatsEntry ::= SEQUENCE {
    bwSCFProcessorCommonCommStatsIndex                    Unsigned32,
    bwSCFProcessorCommonCommHost                          DisplayString,
    bwSCFProcessorCommonCommInterface                     DisplayString,
    bwSCFProcessorCommonCommProtocol                      DisplayString,
    bwSCFProcessorCommonCommAcceptedOutboundConnections   Counter32,
    bwSCFProcessorCommonCommAcceptedInboundConnections    Counter32,
    bwSCFProcessorCommonCommRejectedOutboundConnections   Counter32,
    bwSCFProcessorCommonCommRejectedInboundConnections    Counter32,
    bwSCFProcessorCommonCommOutputMessagesProcessed       Counter32,
    bwSCFProcessorCommonCommInputMessagesProcessed        Counter32,
    bwSCFProcessorCommonCommOutputCommunicationErrors     Counter32,
    bwSCFProcessorCommonCommInputCommunicationErrors      Counter32
  }

  bwSCFProcessorCommonCommStatsIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This integer represents an index in table bwSCFProcessorCommonCommStatsTable. The index represents a connection for a specific Host-Interfacece-Protocol combination."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 1 }

  bwSCFProcessorCommonCommHost OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The address of a host connected to the BroadWorks Common Communication Protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 2 }

  bwSCFProcessorCommonCommInterface OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The IP address of a local interface that a protocol is bound to."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 3 }

  bwSCFProcessorCommonCommProtocol OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The name of a BroadWorks Common Communication Protocol client protocol, e.g. OCI or SyncApi."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 4 }

  bwSCFProcessorCommonCommAcceptedOutboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client in this process connects to the framework to a remote host, using the interface and protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 5 }

  bwSCFProcessorCommonCommAcceptedInboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client in this process connects to the framework from a remote host, using the interface and protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 6 }

  bwSCFProcessorCommonCommRejectedOutboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client connection request going from this process to a remote host and going through this interface has been rejected for a particular protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 7 }

  bwSCFProcessorCommonCommRejectedInboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client connection request coming to this host and going through this interface has been rejected by the framework for a particular protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 8 }

  bwSCFProcessorCommonCommOutputMessagesProcessed OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message has been sent and processed for this host, interface and protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 9 }

  bwSCFProcessorCommonCommInputMessagesProcessed OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message has been received and processed for this host, interface and protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 10 }

  bwSCFProcessorCommonCommOutputCommunicationErrors OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message could not be read because the TCP stack returned an error."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 11 }

  bwSCFProcessorCommonCommInputCommunicationErrors OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message has been sent and processed for this host, interface and protocol."
-- @feature:158606
    ::= { bwSCFProcessorCommonCommStatsEntry 12 }

