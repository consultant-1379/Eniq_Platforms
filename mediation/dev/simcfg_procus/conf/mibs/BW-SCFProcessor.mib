    ::= { bwSS7CapMessagesEntry 2 }

  bwSS7CapMessageTx OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This counter reflects the total number of messages transmitted indexed by opcode."
-- @feature:158606
    ::= { bwSS7CapMessagesEntry 3 }

  bwSS7CapMessageResults OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This counter reflects the total number of positive responses transmitted or received indexed by opcode."
-- @feature:158606
    ::= { bwSS7CapMessagesEntry 4 }

  bwSS7CapMessageErrors OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
