bwDBSGroups		OBJECT IDENTIFIER
    ::= { bwDbsMibConformance 1 }
bwDBSCompliancy    OBJECT IDENTIFIER
    ::= { bwDbsMibConformance 2 }

-- units of conformance
 bwDBSGroupInfo OBJECT-GROUP
    OBJECTS     {  bwInfoDbId, bwInfoDbUniqueName, bwInfoUptime,  bwInfoRole, bwInfoOpenMode, bwInfoLogMode,
                   bwInfoDataguardBroker, bwInfoSwitchOverStatus, bwInfoFastStartFailOverStatus }
    STATUS      current
    DESCRIPTION
        "This group defines the general information for the Database Server database."
    ::= { bwDBSGroups 1 }

 bwDBSGroupSga OBJECT-GROUP
    OBJECTS     { bwFixedSize, bwVariableSize,bwDatabaseBuffers,bwRedoBuffers }
    STATUS      current
    DESCRIPTION
        "This group defines the SGA information for the Database Server database."
    ::= { bwDBSGroups 2 }

 bwDBSGroupError OBJECT-GROUP
    OBJECTS     { bwDbaErrorObjectOwner, bwDbaErrorObjectName, bwDbaErrorObjectType, bwDbaErrorAttribute, bwDbaErrorMessageNumber } 
    STATUS      current
    DESCRIPTION
        "This group defines the error information for the Database Server database."
    ::= { bwDBSGroups 3 }

  bwDBSGroupUserStats OBJECT-GROUP
    OBJECTS     { bwUserStatsReset, bwSQLNetRoundtripsToFromClient, bwConcurrencyWaitTime, bwApplicationWaitTime, bwUserIOWaitTime }
    STATUS      current
    DESCRIPTION
        "This group defines the user statistics for the Database Server database."
    ::= { bwDBSGroups 4 }

  bwDBSGroupRedoStats OBJECT-GROUP
    OBJECTS     { bwRedoStatsReset, bwRedoLogSpaceRequests, bwRedoLogSpaceWaitTime }
    STATUS      current
    DESCRIPTION
        "This group defines the redo statistics for the Database Server database."
    ::= { bwDBSGroups 5 }

  bwDBSGroupEnqueueStats OBJECT-GROUP
    OBJECTS     { bwEnqueueStatsReset, bwEnqueueWaits, bwEnqueueDeadlocks }
    STATUS      current
    DESCRIPTION
        "This group defines the cache statistics for the Database Server database."
    ::= { bwDBSGroups 6 }

  bwDBSGroupCacheStats OBJECT-GROUP
    OBJECTS     { bwCacheStatsReset, bwDbBlockGets,  bwConsistentGets, bwPhysicalReads, bwPhysicalReadIORequests,
    bwDbBlockChanges, bwPhysicalWrites, bwDBWRCheckpointBuffersWritten, bwDBWRCheckpoints }
    STATUS      current
    DESCRIPTION
        "This group defines the cache statistics for the Database Server database."
    ::= { bwDBSGroups 7 }

 bwDBSGroupSqlStats OBJECT-GROUP
    OBJECTS     { bwSqlStatsReset, bwWorkareaExecutionsMultipass, bwParseTimeCpu, bwParseTimeElapsed, bwParseCountTotal,
                  bwSortsMemory, bwSortsDisk,  bwSortsRows, bwTableFetchContinuedRow, bwIndexFastFullScansFull,
                  bwTableScansShortTables, bwTableScansLongTables }
    STATUS      current
    DESCRIPTION
        "This group defines the sql statistics for the Database Server database."
    ::= { bwDBSGroups 8 }

 bwDBSGroupRacStats OBJECT-GROUP
    OBJECTS     { bwRacStatsReset, bwClusterWaitTime }
    STATUS      current
    DESCRIPTION
        "This group defines the rac statistics for the Database Server database."
    ::= { bwDBSGroups 9 }

 bwDBSGroupFileIOStats OBJECT-GROUP
    OBJECTS     {   bwFileIOStatsReset, bwFileIOStatsFileID, bwFileIOStatsFileName, bwFileIOStatsTableSpaceName,
                    bwFileIOStatsPhysicalReads, bwFileIOStatsPhysicalWrites, bwFileIOStatsPhysicalBlockReads, bwFileIOStatsOptimizedPhysReads,
                    bwFileIOStatsPhysBlkWrites, bwFileIOStatsPhysSingleBlkReads   }
    STATUS      current
    DESCRIPTION
        "This group defines the file statistics for the Database Server database."
    ::= { bwDBSGroups 10 }

 bwDBSGroupDebugStats OBJECT-GROUP
    OBJECTS     { bwDebugStatsReset, bwLeafNodeSplits, bwLeafNode9010Splits }
    STATUS      current
    DESCRIPTION
        "This group defines the debug statistics for the Database Server database."
    ::= { bwDBSGroups 11 }

  bwDbsBasicCompliance MODULE-COMPLIANCE
    STATUS      current
    DESCRIPTION
        "BroadWorks Database Server MIB Module compliance"

    MODULE      -- this module

        MANDATORY-GROUPS {  bwDBSGroupInfo, bwDBSGroupSga, bwDBSGroupError, bwDBSGroupUserStats, bwDBSGroupRedoStats, bwDBSGroupEnqueueStats,
                            bwDBSGroupCacheStats , bwDBSGroupSqlStats, bwDBSGroupFileIOStats, bwDBSGroupRacStats, bwDBSGroupDebugStats}

    ::= { bwDBSCompliancy 1 }

END
