  bwSqlStatsReset         OBJECT-TYPE
     SYNTAX               Gauge32
     MAX-ACCESS           read-write
     STATUS               current
     DESCRIPTION          "When set to a value other then 0, all the statistics stored within the sql submodule are reset to 0."
     ::= { sql 1 }

  bwWorkareaExecutionsMultipass OBJECT-TYPE
      SYNTAX                    Counter64  
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Number of workarea execution that requires multiple passes because the work area size is too small."
      ::=  {  sql 2  }

  bwParseTimeCpu                OBJECT-TYPE
      SYNTAX                    TimeInterval  (0..2147483647)
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Total CPU time used for parsing (hard and soft). In units of 0.01 seconds."
      ::=  {  sql 3  }

  bwParseTimeElapsed            OBJECT-TYPE
      SYNTAX                    TimeInterval  (0..2147483647)
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Total elapsed time for parsing. In units of 0.01 seconds."
      ::=  {  sql 4  }

  bwParseCountTotal             OBJECT-TYPE
      SYNTAX                    Counter64  
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Total number of parse calls (hard, soft, and describe). A soft parse is a check on an object already in the shared pool, to verify that the permissions on the underlying object have not changed."
      ::=  {  sql 5  }

  bwSortsMemory                 OBJECT-TYPE
      SYNTAX                    Counter64  
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Number of sort operations that were performed completely in memory and did not require any disk writes."
      ::=  {  sql 6  }

  bwSortsDisk                   OBJECT-TYPE
      SYNTAX                    Counter64  
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Number of sort operations that required at least one disk write.Sorts that require I/O to disk are quite resource intensive."
      ::=  {  sql 7  }

  bwSortsRows                   OBJECT-TYPE
      SYNTAX                    Counter64  
      MAX-ACCESS                read-only
      STATUS                    current
      DESCRIPTION               "Total number of rows sorted."
      ::=  {  sql 8  }

  bwTableFetchContinuedRow    OBJECT-TYPE
      SYNTAX                  Counter64  
      MAX-ACCESS              read-only
      STATUS                  current
      DESCRIPTION             "Number of times a chained or migrated row is encountered during a fetch.Retrieving rows that span more than one block increases the logical I/O by a factor that corresponds to the number of blocks than need to be accessed. Exporting and re-importing may eliminate this problem. Evaluate the settings for the storage parameters PCTFREE and PCTUSED. This problem cannot be fixed if rows are larger than database blocks (for example, if the LONG datatype is used and the rows are extremely large)."
      ::=  {  sql 9  }

  bwIndexFastFullScansFull    OBJECT-TYPE
      SYNTAX                  Counter64  
      MAX-ACCESS              read-only
      STATUS                  current
      DESCRIPTION             "Number of fast full scans initiated for full segments"
      ::=  {  sql 10 }

  bwTableScansShortTables     OBJECT-TYPE
      SYNTAX                  Counter64  
      MAX-ACCESS              read-only
      STATUS                  current
      DESCRIPTION             "Number of table scans performed on short tables."
      ::=  {  sql 11  }

  bwTableScansLongTables      OBJECT-TYPE
      SYNTAX                  Counter64  
      MAX-ACCESS              read-only
      STATUS                  current
      DESCRIPTION             "Number of table scans performed on long tables."
      ::=  {  sql 12  }

