BW-SoftwareManagement DEFINITIONS ::= BEGIN
  IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
      FROM SNMPv2-CONF
    enterprises, MODULE-IDENTITY, OBJECT-TYPE, Unsigned32, Counter32, Gauge32, Integer32
      FROM SNMPv2-SMI
    DateAndTime, DisplayString, TimeStamp
      FROM SNMPv2-TC;

  broadsoft MODULE-IDENTITY
    LAST-UPDATED  "201511060000Z"
    ORGANIZATION  "BroadSoft, Inc."
    CONTACT-INFO  "BroadSoft, Inc.
9737 Washingtonian Blvd
Suite 350
Gaithersburg, MD 20878
Tel: +1-301-977-9440"
    DESCRIPTION
        "This is the MIB for the BroadWorks Software Management application."
    REVISION    "201511060000Z"
                DESCRIPTION    "Creation."
    ::=  {  enterprises 6431  }

  broadworks OBJECT IDENTIFIER
    ::=  {  broadsoft 1  }

  softwareManagement OBJECT IDENTIFIER
    ::=  {  broadworks 60  }

  executors OBJECT IDENTIFIER
    ::=  {  softwareManagement 2  }

  ssh OBJECT IDENTIFIER
    ::=  {  softwareManagement 3  }

-- MIB Compliance - Root
  bwSoftwareManagementMibConformance OBJECT IDENTIFIER
    ::=  {  softwareManagement 1000  }

-- MIB Compliance - Groups
  bwSoftwareManagementMibGroups OBJECT IDENTIFIER
    ::=  {  bwSoftwareManagementMibConformance 1  }

-- MIB Compliance - Module
  bwSoftwareManagementMibCompliancy OBJECT IDENTIFIER
    ::=  {  bwSoftwareManagementMibConformance 2  }


-- ################### ...............................
-- broadsoft(6431).broadworks(1).softwareManagement(60).executors(2)
-- ################### ...............................

  bwSMExecutorReset OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
      "When set to a value other than '0', all statistics stored under executorTable are reset to '0'."
-- @feature:217550
    ::=  {  executors 1  }

  bwSMExecutorTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BwSMExecutorTableEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table exposes the SoftwareManagement executor statistics."
    ::=  {  executors 2  }

  bwSMExecutorTableEntry OBJECT-TYPE
    SYNTAX      BwSMExecutorTableEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
      "This row contains information about a SoftwareManagement executor."
    INDEX       { bwSMExecutorIndex }
    ::=  {  bwSMExecutorTable 1  }

  BwSMExecutorTableEntry ::= SEQUENCE {
    bwSMExecutorIndex  Unsigned32,
    bwSMExecutorName  DisplayString,
    bwSMExecutorQueueCapacity  Gauge32,
    bwSMExecutorQueueSize  Gauge32,
    bwSMExecutorQueueWaitingTimeAvg  Gauge32,
    bwSMExecutorQueueWaitingTimeMin  Gauge32,
    bwSMExecutorQueueWaitingTimeMax  Gauge32,
    bwSMExecutorQueueWaitingTimeMaxTimestamp  DateAndTime,
    bwSMExecutorMinPoolSize  Gauge32,
    bwSMExecutorMaxPoolSize  Gauge32,
    bwSMExecutorThreadsBusy  Gauge32,
    bwSMExecutorCompletedTaskCount  Counter32,
    bwSMExecutorThreadsAlive  Gauge32,
    bwSMExecutorThreadsAliveMax  Gauge32,
    bwSMExecutorTaskProcessingTimeAvg  Gauge32,
    bwSMExecutorTaskProcessingTimeMin  Gauge32,
    bwSMExecutorTaskProcessingTimeMax  Gauge32,
    bwSMExecutorTaskProcessingTimeMaxTimestamp  DateAndTime
  }

  bwSMExecutorIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This integer represents an index to a given executor."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 1  }

  bwSMExecutorName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the name of this executor."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 2  }

  bwSMExecutorQueueCapacity OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the initial capacity of the executor queue."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 3  }

  bwSMExecutorQueueSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the current size of the executor queue."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 4  }

  bwSMExecutorQueueWaitingTimeAvg OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the average time spent waiting in the executor queue since the last reset."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 5  }

  bwSMExecutorQueueWaitingTimeMin OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the minimum amount of time spent waiting in the executor queue since the last reset."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 6  }

  bwSMExecutorQueueWaitingTimeMax OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the maximum amount of time spent waiting in the executor queue since the last reset."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 7  }

  bwSMExecutorQueueWaitingTimeMaxTimestamp OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the time stamp indicating when executorQueueWaitingTime was obtained."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 8  }

  bwSMExecutorMinPoolSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the number of threads to keep in the pool, even if they are idle."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 9  }

  bwSMExecutorMaxPoolSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the maximum number of threads to allow in the pool."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 10  }

  bwSMExecutorThreadsBusy OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the approximate number of threads that are actively executing tasks."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 11  }

  bwSMExecutorCompletedTaskCount OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the approximate total number of tasks that have completed execution.  Because the states of tasks and threads may change dynamically during computation, the returned value is only an approximation; however, it does not ever decrease across successive calls."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 12  }

  bwSMExecutorThreadsAlive OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the current number of threads instantiated."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 13  }

  bwSMExecutorThreadsAliveMax OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the maximum number of threads that can be allocated at the same time."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 14  }

  bwSMExecutorTaskProcessingTimeAvg OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the average processing time spent by this executor."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 15  }

  bwSMExecutorTaskProcessingTimeMin OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the minimum amount of processing time spent by this executor."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 16  }

  bwSMExecutorTaskProcessingTimeMax OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the maximum amount of processing time spent by this executor."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 17  }

  bwSMExecutorTaskProcessingTimeMaxTimestamp OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the time stamp indicating when taskProcessingTimeMax was obtained."
-- @feature:217550
    ::=  {  bwSMExecutorTableEntry 18  }

