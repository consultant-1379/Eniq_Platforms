-- BroadWorks MIB Conformance Definition  ...........................................

  bwsoftwaremanagementExecutorsGroup OBJECT-GROUP
    OBJECTS { bwSMExecutorReset, bwSMExecutorIndex, bwSMExecutorName, bwSMExecutorQueueCapacity, bwSMExecutorQueueSize, bwSMExecutorQueueWaitingTimeAvg, bwSMExecutorQueueWaitingTimeMin, bwSMExecutorQueueWaitingTimeMax, bwSMExecutorQueueWaitingTimeMaxTimestamp, bwSMExecutorMinPoolSize, bwSMExecutorMaxPoolSize, bwSMExecutorThreadsBusy, bwSMExecutorCompletedTaskCount, bwSMExecutorThreadsAlive, bwSMExecutorThreadsAliveMax, bwSMExecutorTaskProcessingTimeAvg, bwSMExecutorTaskProcessingTimeMin, bwSMExecutorTaskProcessingTimeMax, bwSMExecutorTaskProcessingTimeMaxTimestamp }
    STATUS  current
    DESCRIPTION "This group defines the statistics for softwareManagement executors."
    ::=  {  bwSoftwareManagementMibGroups 1  }

  bwsoftwaremanagementSshGroup OBJECT-GROUP
    OBJECTS { bwSMSshResetStats, bwSMSshSessionConnectionRequests, bwSMSshSessionConnectedEvents, bwSMSshSessionDisconnectedEvents, bwSMSshSessionAuthenticationFailureEvents, bwSMSshSessionConnectionFailureEvents, bwSMSshChannelOpenRequestEvents, bwSMSshChannelOpenedEvents, bwSMSshChannelOpenFailedEvents, bwSMSshChannelInputAvailableEvents, bwSMSshChannelOutputReadyEvents, bwSMSshChannelDisconnectedEvents, bwSMSshCommandStartedEvents, bwSMSshCommandFailedEvents, bwSMSshCommandTimeoutEvents, bwSMSshCommandCompletedEvents, bwSMSshCmdAvgConnectionTime, bwSMSshCmdMinConnectionTime, bwSMSshCmdMaxConnectionTime, bwSMSshCmdTotalConnectionTime, bwSMSshCmdAvgProcessingTime, bwSMSshCmdMinProcessingTime, bwSMSshCmdMaxProcessingTime, bwSMSshCmdTotalProcessingTime }
    STATUS  current
    DESCRIPTION "This group defines the statistics for softwareManagement ssh."
    ::=  {  bwSoftwareManagementMibGroups 2  }

  bwSoftwareManagementBasicCompliance MODULE-COMPLIANCE
    STATUS      current
    DESCRIPTION
        "BW-SoftwareManagement MIB compliance."
    MODULE
      MANDATORY-GROUPS { bwsoftwaremanagementExecutorsGroup, bwsoftwaremanagementSshGroup }
    ::=  {  bwSoftwareManagementMibCompliancy 1  }

END
