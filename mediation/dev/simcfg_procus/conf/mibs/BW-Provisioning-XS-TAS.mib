    bwProvCommonCommAcceptedOutboundConnections   Counter32,
    bwProvCommonCommAcceptedInboundConnections    Counter32,
    bwProvCommonCommRejectedOutboundConnections   Counter32,
    bwProvCommonCommRejectedInboundConnections    Counter32,
    bwProvCommonCommOutputMessagesProcessed       Counter32,
    bwProvCommonCommInputMessagesProcessed        Counter32,
    bwProvCommonCommOutputCommunicationErrors     Counter32,
    bwProvCommonCommInputCommunicationErrors      Counter32
  }

  bwProvCommonCommStatsIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "This integer represents an index in table bwProvCommonCommStatsTable. The index represents a connection for a specific Host-Interfacece-Protocol combination."
    ::= { bwProvCommonCommStatsEntry 1 }

  bwProvCommonCommHost OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The address of a host connected to the BroadWorks Common Communication Protocol."
    ::= { bwProvCommonCommStatsEntry 2 }

  bwProvCommonCommInterface OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The IP address of a local interface that a protocol is bound to."
    ::= { bwProvCommonCommStatsEntry 3 }

  bwProvCommonCommProtocol OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "The name of a BroadWorks Common Communication Protocol client protocol, e.g. OCI or SyncApi."
    ::= { bwProvCommonCommStatsEntry 4 }

  bwProvCommonCommAcceptedOutboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client in this process connects to the framework to a remote host, using the interface and protocol."
    ::= { bwProvCommonCommStatsEntry 5 }

  bwProvCommonCommAcceptedInboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client in this process connects to the framework from a remote host, using the interface and protocol."
    ::= { bwProvCommonCommStatsEntry 6 }

  bwProvCommonCommRejectedOutboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client connection request going from this process to a remote host and going through this interface has been rejected for a particular protocol."
    ::= { bwProvCommonCommStatsEntry 7 }

  bwProvCommonCommRejectedInboundConnections OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a client connection request coming to this host and going through this interface has been rejected by the framework for a particular protocol."
    ::= { bwProvCommonCommStatsEntry 8 }

  bwProvCommonCommOutputMessagesProcessed OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message has been sent and processed for this host, interface and protocol."
    ::= { bwProvCommonCommStatsEntry 9 }

  bwProvCommonCommInputMessagesProcessed OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message has been received and processed for this host, interface and protocol."
    ::= { bwProvCommonCommStatsEntry 10 }

  bwProvCommonCommOutputCommunicationErrors OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message could not be read because the TCP stack returned an error."
    ::= { bwProvCommonCommStatsEntry 11 }

  bwProvCommonCommInputCommunicationErrors OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This counter is incremented whenever a message has been sent and processed for this host, interface and protocol."
    ::= { bwProvCommonCommStatsEntry 12 }

