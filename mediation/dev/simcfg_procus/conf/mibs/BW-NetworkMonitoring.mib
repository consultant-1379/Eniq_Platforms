    STATUS      current
    DESCRIPTION
      "This integer represents an index to a given executor."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 1  }

  bwNMControllerExecutorName OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the name of this executor."
