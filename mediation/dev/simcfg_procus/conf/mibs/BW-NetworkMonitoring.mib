BW-NetworkMonitoring DEFINITIONS ::= BEGIN
  IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
      FROM SNMPv2-CONF
    enterprises, MODULE-IDENTITY, OBJECT-TYPE, Unsigned32, Counter32, Gauge32, Integer32
      FROM SNMPv2-SMI
    DateAndTime, DisplayString, TimeStamp
      FROM SNMPv2-TC;

  broadsoft MODULE-IDENTITY
    LAST-UPDATED  "201604060000Z"
    ORGANIZATION  "BroadSoft, Inc."
    CONTACT-INFO  "BroadSoft, Inc.
9737 Washingtonian Blvd
Suite 350
Gaithersburg, MD 20878
Tel: +1-301-977-9440"
    DESCRIPTION
        "This is the MIB for the BroadWorks Network Monitoring application."
    REVISION    "201604060000Z"
                DESCRIPTION    "Added High Availability."
    REVISION    "201502060000Z"
                DESCRIPTION    "Updated description for JVM counters."
    REVISION    "201411040000Z"
                DESCRIPTION    "Added counters for heap, nonheap and garbageCollection Resources."
    REVISION    "201409100000Z"
                DESCRIPTION    "Creation."
    ::=  {  enterprises 6431  }

  broadworks OBJECT IDENTIFIER
    ::=  {  broadsoft 1  }

  networkMonitoring OBJECT IDENTIFIER
    ::=  {  broadworks 58  }

  executors OBJECT IDENTIFIER
    ::=  {  networkMonitoring 1  }

  databaseStats OBJECT IDENTIFIER
    ::=  {  networkMonitoring 2  }

  replication OBJECT IDENTIFIER
    ::=  {  networkMonitoring 3  }

-- MIB Compliance - Root
  bwNMControllerMibConformance OBJECT IDENTIFIER
    ::=  {  networkMonitoring 1000  }

  bwNMControllerMariaDB OBJECT IDENTIFIER
    ::=  {  databaseStats 1  }

  bwNMControllerPostgreSQL OBJECT IDENTIFIER
    ::=  {  databaseStats 2  }

  primary OBJECT IDENTIFIER
    ::=  {  replication 1  }

  standby OBJECT IDENTIFIER
    ::=  {  replication 2  }

-- MIB Compliance - Groups
  bwNMControllerMibGroups OBJECT IDENTIFIER
    ::=  {  bwNMControllerMibConformance 1  }

-- MIB Compliance - Module
  bwNMControllerMibCompliancy OBJECT IDENTIFIER
    ::=  {  bwNMControllerMibConformance 2  }


