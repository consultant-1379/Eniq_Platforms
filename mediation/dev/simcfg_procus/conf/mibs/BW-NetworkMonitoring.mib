    ::=  {  bwNMControllerExecutorTableEntry 13  }

  bwNMControllerExecutorThreadsAliveMax OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the maximum number of threads that can be allocated at the same time."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 14  }

  bwNMControllerExecutorTaskProcessingTimeAvg OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the average processing time spent by this executor."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 15  }

  bwNMControllerExecutorTaskProcessingTimeMin OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the minimum amount of processing time spent by this executor."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 16  }

  bwNMControllerExecutorTaskProcessingTimeMax OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the maximum amount of processing time spent by this executor."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 17  }

  bwNMControllerExecutorTaskProcessingTimeMaxTimestamp OBJECT-TYPE
    SYNTAX      DateAndTime
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the time stamp indicating when taskProcessingTimeMax was obtained."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 18  }

