  bwNMControllerExecutorReset OBJECT-TYPE
    SYNTAX      Counter32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
      "When set to a value other than '0', all statistics stored under executorTable are reset to '0'."
-- @feature:6984
    ::=  {  executors 1  }

  bwNMControllerExecutorTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF BwNMControllerExecutorTableEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
    "This table exposes the NetworkMonitoring executor statistics."
-- @feature:6984
    ::=  {  executors 2  }

  bwNMControllerExecutorTableEntry OBJECT-TYPE
    SYNTAX      BwNMControllerExecutorTableEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
      "This row contains information about a NetworkMonitoring executor."
    INDEX       { bwNMControllerExecutorIndex }
-- @feature:6984
    ::=  {  bwNMControllerExecutorTable 1  }

  BwNMControllerExecutorTableEntry ::= SEQUENCE {
    bwNMControllerExecutorIndex  Unsigned32,
    bwNMControllerExecutorName  DisplayString,
    bwNMControllerExecutorQueueCapacity  Gauge32,
    bwNMControllerExecutorQueueSize  Gauge32,
    bwNMControllerExecutorQueueWaitingTimeAvg  Gauge32,
    bwNMControllerExecutorQueueWaitingTimeMin  Gauge32,
    bwNMControllerExecutorQueueWaitingTimeMax  Gauge32,
    bwNMControllerExecutorQueueWaitingTimeMaxTimestamp  DateAndTime,
    bwNMControllerExecutorMinPoolSize  Gauge32,
    bwNMControllerExecutorMaxPoolSize  Gauge32,
    bwNMControllerExecutorThreadsBusy  Gauge32,
    bwNMControllerExecutorCompletedTaskCount  Counter32,
    bwNMControllerExecutorThreadsAlive  Gauge32,
    bwNMControllerExecutorThreadsAliveMax  Gauge32,
