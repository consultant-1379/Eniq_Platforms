  bwNMControllerExecutorQueueSize OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the current size of the executor queue."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 4  }

  bwNMControllerExecutorQueueWaitingTimeAvg OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the average time spent waiting in the executor queue since the last reset."
-- @feature:6984
    ::=  {  bwNMControllerExecutorTableEntry 5  }

  bwNMControllerExecutorQueueWaitingTimeMin OBJECT-TYPE
    SYNTAX      Gauge32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
      "This is the minimum amount of time spent waiting in the executor queue since the last reset."
